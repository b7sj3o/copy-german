u043a\u0443<\/strong> \u0439 \u043f\u0440\u043e\u0432\u043e\u0434\u0438\u043c\u043e \u0440\u0456\u0437\u043d\u043e\u043c\u0430\u043d\u0456\u0442\u043d\u0456 \u0437\u0430\u0445\u043e\u0434\u0438 \u0434\u043b\u044f \u043d\u0430\u0448\u0438\u0445 \u0441\u0442\u0443\u0434\u0435\u043d\u0442\u0456\u0432, \u0437\u0443\u0441\u0442\u0440\u0456\u0447\u0456 \u0437 \u043a\u0440\u0443\u0442\u0438\u043c\u0438 \u043f\u0440\u043e\u0444\u0456, \u0431\u0456\u0437\u043d\u0435\u0441\u043c\u0435\u043d\u0430\u043c\u0438 \u0442\u043e\u0449\u043e.<\/p>\n\n\t\t        \t<\/div>\n\t\t       \t<\/div><\/div><script type=\"text\/javascript\">\n\t\t\t\tvar study_carousel = document.querySelector(\".study-carousel\");\n\t\t\t\tvar flkty = new Flickity( study_carousel, {\n\t\t\t\t  \/\/ options\n\t\t\t\t  cellAlign: \"left\",\n\t\t\t\t  contain: false,\n\t\t\t\t  adaptiveHeight: true,\n\t\t\t\t  \/\/ setGallerySize: false,\n\t\t\t\t  dragThreshold: 45,\n\t\t\t\t  selectedAttraction: 0.2,\n\t\t\t\t  friction: 0.8\n\t\t\t\t});\n\t\t\t<\/script>\t\t<div class=\"elementor-shortcode\"><\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<section class=\"elementor-section elementor-inner-section elementor-element elementor-element-17aecfb elementor-section-full_width elementor-hidden-tablet elementor-hidden-mobile elementor-section-height-default elementor-section-height-default\" data-id=\"17aecfb\" data-element_type=\"section\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-33 elementor-inner-column elementor-element elementor-element-315f760\" data-id=\"315f760\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-d9e0793 elementor-widget__width-initial elementor-widget elementor-widget-heading\" data-id=\"d9e0793\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"e